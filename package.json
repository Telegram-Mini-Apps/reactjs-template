    "eruda": "^3.4.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.534.0",
    "mapbox-gl": "^3.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.23.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20.12.11",
